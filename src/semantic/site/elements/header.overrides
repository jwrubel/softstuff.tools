// h1 should be in libel suit, all caps.
// but not subheaders
h1.ui.header {
  font-family: @h1Font;
  text-transform: uppercase;
  font-weight: normal;
}
h1.ui.header .sub.header {
  font-family: @headerFont;
  text-transform: uppercase;
  font-weight: @fontWeight;
}